# OpenManus Environment Variables

# API Configuration
VITE_API_URL=https://api.openmanus.app
VITE_API_VERSION=v1

# LLM Configuration
VITE_LLM_PROVIDER=openrouter
VITE_LLM_API_KEY=your_api_key_here
VITE_LLM_MODEL=deepseek-coder

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_BROWSER_AUTOMATION=false

# Deployment
VITE_DEFAULT_DEPLOYMENT_PLATFORM=vercel